{
  "name": "[无码]集中营",
  "base_url": "https://heimao.jzy01.cc",
  "action_data": {
    "video_url": {
      "steps": [
        {
          "action": "request_redirect",
          "url": "{{base_url}}",
          "priority_scraper": true,
          "use_js": true,
          "timeout": 15
        }
      ],
      "result": {
        "domains": "{{real_domain}}",
        "wd_url": "/play/yjs/<*>-uncensored-leak.html?ref=HEIMAO"
      },
      "logic": {
        "platform_names": [
          "[无码]集中营",
          "jzyu"
        ],
        "invalid": [
          {
            "type": "html",
            "selector": "div.mymenu",
            "empty": true
          },
          {
            "type": "html",
            "selector": "div.mymenu",
            "match": "{{keyword}}",
            "method": "contains",
            "invert": true
          }
        ],
        "valid": [
          {
            "type": "html",
            "selector": "title, h1, div.mymenu",
            "match": "{{keyword}}",
            "method": "contains"
          },
          {
            "type": "link",
            "match": "video",
            "method": "contains"
          },
          {
            "type": "html",
            "selector": "video, iframe",
            "min_count": 1
          }
        ]
      }
    }
  }
}