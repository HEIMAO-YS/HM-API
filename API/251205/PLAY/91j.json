{
  "name": "91RJAV",
  "base_url": "https://urlwww.top/7htcl",
  "action_data": {
    "video_url": {
      "steps": [
        {
          "action": "request_redirect",
          "url": "{{base_url}}",
          "priority_scraper": true,
          "use_js": true,
          "max_retries": 2,
          "timeout": 15
        }
      ],
      "result": {
        "domains": "{{real_domain}}",
        "wd_url": "/<*>/?ref=HEIMAO"
      },
      "logic": {
        "platform_names": [
          "91RJAV"
        ],
        "invalid": [
          {
            "type": "html",
            "selector": ".widget-title .count",
            "match": "(0 results)",
            "method": "contains"
          },
          {
            "type": "html",
            "selector": "title",
            "match": "未找到页面 - 91JAV",
            "method": "contains"
          },
          {
            "type": "html",
            "selector": ".page-header .page-title",
            "match": "Oops! That page can’t be found.",
            "method": "contains"
          }
        ],
        "valid": [
          {
            "type": "html",
            "selector": "title",
            "match": "{{keyword}}",
            "method": "contains"
          },
          {
            "type": "html",
            "selector": ".title",
            "match": "{{keyword}}",
            "method": "contains"
          }
        ]
      }
    }
  }
}