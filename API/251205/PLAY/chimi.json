{
  "name": "痴迷AV",
  "base_url": "https://gitlab.com/chimi2025-url/chimi2025/-/raw/main/README.md",
  "action_data": {
    "video_url": {
      "steps": [
        {
          "action": "request_js",
          "url": "{{base_url}}",
          "priority_scraper": true,
          "max_retries": 2,
          "timeout": 15
        },
        {
          "action": "parse_text",
          "content_type": "markdown",
          "regex": "最新地址（无需翻墙）：\\s*(https?://[^\\s]+)",
          "group_index": 1,
          "save_as": "domain_list",
          "multiple": false
        }
      ],
      "result": {
        "domains": [
          "https://chimiav.com",
          "https://chimi85.cc",
          "https://chimi86.cc",
          "https://chimi87.cc",
          "https://chimi88.cc",
          "https://chimi89.cc",
          "https://chimi90.cc",
          "https://chimi91.cc",
          "https://chimi92.cc",
          "https://chimi93.cc",
          "https://chimi94.cc",
          "https://chimi95.cc",
          "{{domain_list}}"
        ],
        "wd_url": "/index.php/vod/search.html?wd=<*>"
      },
      "logic": {
        "platform_names": [
          "痴迷AV"
        ],
        "invalid": [
          {
            "type": "html",
            "selector": ".list_box a",
            "match": "{{keyword}}",
            "method": "contains",
            "invalid_on_not_found": true
          }
        ],
        "valid": [
          {
            "type": "html",
            "selector": ".list_box a",
            "match": "{{keyword}}",
            "method": "contains"
          }
        ]
      }
    }
  }
}