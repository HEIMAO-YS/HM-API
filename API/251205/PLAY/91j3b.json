{
  "name": "91JAV备用",
  "base_url": "https://gitlab.com/91JAV1/dz/-/raw/main/readme.md",
  "action_data": {
    "video_url": {
      "steps": [
        {
          "action": "request_js",
          "url": "{{base_url}}",
          "priority_scraper": true,
          "max_retries": 2,
          "timeout": 15
        },
        {
          "action": "parse_text",
          "content_type": "markdown",
          "regex": "91JAV 国内备用地址：\\s*(https?://[^\\s]+)",
          "group_index": 1,
          "save_as": "real_domain",
          "multiple": false
        }
      ],
      "result": {
        "domains": "{{real_domain}}",
        "wd_url": "/cn/search/<*>"
      },
      "logic": {
        "platform_names": [
          "91JAV"
        ],
        "invalid": [
          {
            "type": "html",
            "selector": ".title-box .inactive-color",
            "match": "0 部影片",
            "method": "contains"
          }
        ],
        "valid": [
          {
            "type": "html",
            "selector": ".video-img-box .title a, .video-img-box .img-box a",
            "match": "{{keyword}}",
            "method": "contains"
          }
        ]
      }
    }
  }
}