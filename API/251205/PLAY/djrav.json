{
  "name": "东京热AV",
  "base_url": "https://proxy.v2gh.com/https://raw.githubusercontent.com/djrAV-888/djrAV/refs/heads/main/README.md",
  "action_data": {
    "video_url": {
      "enable_js_redirect": false,
      "steps": [
        {
          "action": "request_js",
          "url": "{{base_url}}",
          "max_retries": 1,
          "timeout": 15
        },
        {
          "action": "parse_text",
          "content_type": "markdown",
          "regex": "国内最新网址 \\s*(https?://[^\\s]+)",
          "group_index": 1,
          "save_as": "djr_cn",
          "multiple": false
        },
        {
          "action": "request_js",
          "url": "{{djr_cn}}",
          "force_js": true,
          "timeout": 10
        },
        {
          "action": "select",
          "selector": ".list a",
          "attribute": "href",
          "multiple": true,
          "save_as": "domain_list"
        }
      ],
      "result": {
        "domains": [
          "https://djav18.net",
          "{{domain_list}}"
        ],
        "wd_url": "/search/index/word/<*>"
      },
      "logic": {
        "platform_names": [
          "东京热AV"
        ],
        "invalid": [
          {
            "type": "html",
            "selector": ".inactive-color.fs-2.mb-0",
            "match": "0 部影片",
            "method": "contains"
          }
        ],
        "valid": [
          {
            "type": "html",
            "selector": ".detail h6",
            "match": "{{keyword}}",
            "method": "contains"
          }
        ]
      }
    }
  }
}