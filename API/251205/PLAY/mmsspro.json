{
  "name": "mmss.pro",
  "base_url": "https://chisha.tssuy.cn/fby.php",
  "action_data": {
    "video_url": {
      "steps": [
        {
          "action": "request_js",
          "url": "{{base_url}}",
          "priority_scraper": true,
          "max_retries": 2,
          "timeout": 15
        },
        {
          "action": "parse_json",
          "save_as": "domain_list"
        }
      ],
      "result": {
        "domains": "{{domain_list}}",
        "wd_url": "/index.php/vod/search.html?wd=<*>"
      },
      "logic": {
        "platform_names": [
          "mmss.pro"
        ],
        "invalid": [
          {
            "type": "html",
            "selector": ".pagination .wrap h1",
            "match": "没有找到匹配数据",
            "method": "contains"
          },
          {
            "type": "html",
            "selector": ".main-container .cat_pos_l .mac_total",
            "match": "0",
            "method": "exact"
          }
        ],
        "valid": [
          {
            "type": "html",
            "selector": ".box.movie_list .content-item a",
            "match": "{{keyword}}",
            "method": "contains"
          }
        ]
      }
    }
  }
}